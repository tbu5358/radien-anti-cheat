{
  "name": "radien-anticheat-bot",
  "version": "1.0.0",
  "description": "Anti-Cheat Moderation Bot for Raiden Games",
  "main": "dist/bot.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/bot.js",
    "dev": "ts-node src/bot.ts",
    "dev:mock": "MOCK_MODE=true ts-node src/bot.ts",
    "watch": "nodemon --exec ts-node src/bot.ts",
    "register-commands": "ts-node scripts/register-commands.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "quality": "npm run lint && npm run type-check && npm run test",
    "loadtest": "npx artillery run tests/load/artillery.yml",
    "simulate:events": "ts-node src/scripts/replayAntiCheat.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "anticheat",
    "moderation"
  ],
  "author": "Raiden Games",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "^2.1.0",
    "axios": "^1.13.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
